{
  "name": "rev-api-hapi",
  "version": "0.0.1",
  "description": "RevJS Isomorphic Data Library - API Plugin for HapiJS",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-bin tslint -p . && run-bin tsc",
    "build-watch": "run-bin tsc --watch",
