  "license": "MIT",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "graphql": "^0.10.1",
    "graphql-server-hapi": "^0.8.0",
    "hapi": "^16.0.0",
    "rev-api": "^0.0.1",
    "rev-models": "^0.0.1",
    "tslib": "1.x"
  },
  "devDependencies": {
    "@types/hapi": "^16.0.0",
    "hapi": "^16.0.0",
    "run-bin": "^1.0.0"
  }
